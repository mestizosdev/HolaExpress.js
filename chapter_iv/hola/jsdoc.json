{
    "plugins": ["jsdoc-http-plugin"],
    "source": {
      "include": ["src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules|doc|seeders|migrations)"
    },
    "templates": {
        "name": "HolaExpress.js",
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
      "recurse": "true",
      "template": "node_modules/docdash",
      "destination": "./doc/HolaExpress.js",
      "readme": "./README.md"
    }
  }
  